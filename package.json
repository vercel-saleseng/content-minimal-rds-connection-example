{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:lambda": "tsc -p src/lambda",
    "terraform:init": "cd terraform && terraform init",
    "terraform:plan": "cd terraform && terraform plan --var-file=rds.tfvars",
    "terraform:apply": "cd terraform && terraform apply --var-file=rds.tfvars",
    "terraform:destroy": "cd terraform && terraform destroy --var-file=rds.tfvars",
    "deploy:plan": "pnpm run build:lambda && pnpm run terraform:plan",
    "deploy:apply": "pnpm run build:lambda && pnpm run terraform:apply"
  },
  "dependencies": {
    "@types/pg": "^8.11.6",
    "clsx": "^2.1.1",
    "daisyui": "^4.11.1",
    "next": "14.2.3",
    "pg": "^8.11.5",
    "react": "^18",
    "react-dom": "^18",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.137",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
